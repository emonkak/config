def_key "backspace"
  jump_to_parent_directory

def_key "b"
  dummy

def_key "d"
  delete_playlist_items

def_key "g"
  move_home

def_key "G"
  move_end

def_key "f"
  apply_filter

def_key "h"
  jump_to_parent_directory

def_key "h"
  previous_column

def_key "j"
  scroll_down

def_key "J"
  move_sort_order_down

def_key "J"
  move_selected_items_down

def_key "J"
  jump_to_parent_directory

def_key "k"
  scroll_up

def_key "K"
  move_sort_order_up

def_key "K"
  move_selected_items_up

def_key "l"
  enter_directory

def_key "l"
  next_column

def_key "n"
  next_found_item

def_key "N"
  previous_found_item

def_key "v"
  select_item
  scroll_down

def_key "ctrl-b"
  page_up

def_key "ctrl-f"
  page_down

def_key "ctrl-u"
  scroll_up_album

def_key "ctrl-d"
  scroll_down_album

def_key "ctrl-l"
  dummy

def_key "left"
  seek_backward

def_key "right"
  seek_forward
