+             (list (cons lst '()) (list (list (car lst) (car lst) (car lst))
+                                        (list (cadr lst) (cadr lst) (cadr lst)))))))
+       (filter (lambda (x) (not (string=? "n" x)))
+               (map car ja-consonant-syllable-table))))
