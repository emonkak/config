Rxvt.color6: rgb:00/76/76
Rxvt.color14: rgb:37/bb/bb
! White
Rxvt.color7: rgb:81/81/81
Rxvt.color15: rgb:ff/ff/ff

Rxvt.font: \
  xft:TheSans Mono:pixelsize=14, \
  xft:KaiLunaStd:pixelsize=14, \
  xft:Droid Sans Fallback:pixelsize=14
Rxvt.imFont: \
  -envy-code_b-medium-r-normal--14-130-75-75-c-70-iso8859-1, \
  -mplus-gothic-medium-r-normal--12-120-75-75-c-60-jisx0201.1976-0, \
  -mplus-gothic-medium-r-normal--12-120-75-75-c-120-jisx0208.1990-0

Rxvt.matcher.button: 1
Rxvt.perl-ext-common: default,matcher
Rxvt.urlLauncher: google-chrome

Rxvt.externalBorder: 0
Rxvt.internalBorder: 2
Rxvt.lineSpace: 3
Rxvt.saveLines: 1000
Rxvt.scrollBar: false
Rxvt.secondaryScreen: true
Rxvt.secondaryScroll: true
Rxvt.termName: rxvt-256color
Rxvt.urgentOnBell: true
Rxvt.visualBell: false


