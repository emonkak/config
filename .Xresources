! My Xresources

! XFT
Xft.antialias: 1
Xft.autohint: 0
Xft.hinting: 0
Xft.hintstyle: hintnone
Xft.rgba: rgb


! CURSOR
Xcursor.theme: Vanilla-DMZ
Xcursor.size: 24


! FONT
Rxvt.font: \
  xft:Consolas:pixelsize=14:antialias=true, \
  xft:AXIS Std:pixelsize=14:antialias=true
Rxvt.boldFont: \
  xft:Consolas:pixelsize=14:bold, \
  xft:AXIS Std:pixelsize=14:bold
Rxvt.italicFont: \
  xft:Consolas:pixelsize=14:italic, \
  xft:AXIS Std:pixelsize=14:italic
Rxvt.boldItalicFont: \
  xft:Consolas:pixelsize=14:bold:italic, \
  xft:AXIS Std:pixelsize=14:bold:italic
Rxvt.imFont: \
  -sony-fixed-medium-r-normal--16-*-*-*-*-*-iso8859-1, \
  -sony-fixed-medium-r-normal--16-*-*-*-*-*-jisx0201.1976-0, \
  -jis-fixed-medium-r-normal--16-*-*-*-*-*-jisx0208.1983-0


! COLOR
Rxvt.foreground:   #cccccc
Rxvt.background:   #222222
Rxvt.cursorColor:  #009900
Rxvt.cursorColor2: #000000

Rxvt.color0:  #000000
Rxvt.color1:  #990000
Rxvt.color2:  #009900
Rxvt.color3:  #999900
Rxvt.color4:  #000099
Rxvt.color5:  #990099
Rxvt.color6:  #009999
Rxvt.color7:  #999999
Rxvt.color8:  #555555
Rxvt.color9:  #cc6633
Rxvt.color10: #00cc00
Rxvt.color11: #cccc00
Rxvt.color12: #3366cc
Rxvt.color13: #cc00cc
Rxvt.color14: #00cccc
Rxvt.color15: #ffffff


! MISC
Rxvt.externalBorder: 0
Rxvt.geometry: 80x25
Rxvt.internalBorder: 2
Rxvt.lineSpace: 0
Rxvt.saveLines: 1000
Rxvt.scrollBar: false
Rxvt.secondaryScreen: true
Rxvt.secondaryScroll: true
Rxvt.termName: rxvt-unicode256
Rxvt.visualBell: false
