<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>org.apache.httpd</string>
		<key>RunAtLoad</key>
		<true/>
		<key>KeepAlive</key>
		<true/>
		<key>ProgramArguments</key>
		<array>
			<string>~/Gentoo/usr/sbin/apache2</string>
			<string>-D</string>
			<string>DEFAULT_VHOST</string>
			<string>-k</string>
			<string>start</string>
		</array>
	</dict>
</plist>
